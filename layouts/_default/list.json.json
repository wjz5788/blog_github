[
{{- range .Pages -}}
{
  "title": {{ .Title | jsonify }},
  "url": {{ .RelPermalink | jsonify }},
  "content": {{ .Plain | jsonify }},
  "summary": {{ .Summary | jsonify }},
  "date": {{ .Date.Format "2006-01-02" | jsonify }},
  "tags": {{ .Params.tags | jsonify }}
}{{ if not (last .) }},{{ end }}
{{- end -}}
]
